{
  "name": "tmp",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && PUBLIC_BRANCH=$HEAD astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "jest --watch"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.1",
    "@astrojs/tailwind": "^5.0.2",
    "astro": "^3.4.3",
    "decap-cms-app": "^3.0.12",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "decap-server": "^3.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.1",
    "ts-jest": "^29.1.1"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-astro"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  }
